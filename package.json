{
  "name": "carelytix",
  "version": "0.0.1",
  "scripts": {
    "dev": "pnpm run -r --parallel --aggregate-output dev",
    "build": "pnpm run -r build",
    "db:generate": "pnpm --filter db prisma generate",
    "db:migrate": "pnpm --filter db prisma migrate dev --name init",
    "db:studio": "pnpm --filter db prisma studio",
    "db:deploy": "pnpm --filter db prisma migrate deploy",
    "db:reset": "pnpm --filter db prisma migrate reset --force"
  },
  "devDependencies": {
    "prettier": "^2.6.2"
  },
  "author": "carelytix",
  "packageManager": "pnpm@10.11.0"
}
